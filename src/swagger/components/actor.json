          },
          "name": {
            "type": "string"
          },
          "slug": {
            "type": "string"
          },
          "gender": {
            "type": "string"
          },
          "birthday": {
            "type": "string"
          },
          "avatar": {
            "type": "string"
          },
          "address": {
            "type": "string"
          },
          "descriptions": {
            "type": "string"
          },
          "viewer": {
            "type": "string"
          },
          "created_at": {
            "type": "string"
          },
          "updated_at": {
            "type": "string"
          },
          "deleted_at": {
            "type": "string"
          }
        }
      }
    }
  }
